<!-- GETTING-STARTED -->

<div class="getting-started-container">
  <div class="getting-started-heading">
    <h1>Getting Started</h1>
    <div class="a">
      <p>Welcome to the LIVINGEARTH Interactive map. Please start by Choosing a Country</p>
    </div>
  </div>
  <div class="getting-started-nav">
    <div class="getting-started-middle">
      <div class="getting-started-icon">
        <img src="/assets/icon/questionmark.svg">
      </div>
      <div class="getting-started-content">
        <p>Press this icon to learn about datasets​</p>
      </div>
    </div>
    <div class="getting-started-middle">
      <div class="getting-started-icon">
        <img src="/assets/icon/download.svg">
      </div>
      <div class="getting-started-content">
        <p>Press this icon to download datasets​</p>
      </div>
    </div>
    <div class="getting-started-link">
      <div>
        <a href="/tools/learning-resources">Go to learning resource</a>
        <img src="/assets/icon/arrowright.svg">
      </div>
      <p>At any point you can go to our ​learning page from the toolbar​</p>
    </div>
  </div>
  <div class="getting-started-learning">
    <p class="getting-started-learning-title">Learning Resources</p>
    <div class="getting-started-learning-content">
  {% for item in site.data.getting-started.index %}
    <div class="getting-started-learning-items">
      <div class="img">
        <img src="{{ item.link }}">
      </div>
      <p>{{ item.title }}</p>
    </div>
  {% endfor %}
</div>

  </div>
  <div class="getting-started-end">
    <button id="closeButton" type="submit" data-dismiss="modal">
      <div style="display: flex; margin-top: 15px;">
        <p>Got it</p>
        <svg xmlns="http://www.w3.org/2000/svg" width="17" height="17" viewBox="0 0 17 17" fill="none">
